<?xml version="1.0" encoding="UTF-8" ?>
<LWM2M xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
  <Object ObjectType="MODefinition">
    <ObjectID>3012</ObjectID>
    <ObjectURN>urn:oma:lwm2m:ext:3012</ObjectURN>
    <Name>Scene Control</Name>
    <MultipleInstances>Multiple</MultipleInstances>
    <Mandatory>Optional</Mandatory>
    <Description>Scene Control Object</Description>
    <Resources>
      <Item ID="5075">
        <ResourceID>5075</ResourceID>
        <Name>Current Scene</Name>
        <Operations>R</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Mandatory</Mandatory>
        <Type>Objlink</Type>
        <RangeEnumeration></RangeEnumeration>
        <Units></Units>
        <Description>The most recently recalled Scene</Description>
      </Item>
      <Item ID="5076">
        <ResourceID>5076</ResourceID>
        <Name>Recall Scene</Name>
        <Operations>E</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Mandatory</Mandatory>
        <Type>Objlink</Type>
        <RangeEnumeration></RangeEnumeration>
        <Units></Units>
        <Description>Recall the indicated Scene</Description>
      </Item>
      <Item ID="5002">
        <ResourceID>5002</ResourceID>
        <Name>Transition Time</Name>
        <Operations>RW</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Optional</Mandatory>
        <Type>Float</Type>
        <RangeEnumeration></RangeEnumeration>
        <Units>ucum:%</Units>
        <Description>Time to reach the new state or value</Description>
      </Item>
      <Item ID="5073">
        <ResourceID>5073</ResourceID>
        <Name>Valid</Name>
        <Operations>R</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Optional</Mandatory>
        <Type>Boolean</Type>
        <RangeEnumeration></RangeEnumeration>
        <Units>ucum:%</Units>
        <Description>Indicates that the value obtained is the most current value, e.g. a scene state matches the state of all resources in the scene</Description>
      </Item>
      <Item ID="5077">
        <ResourceID>5077</ResourceID>
        <Name>Update Scene</Name>
        <Operations>E</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Optional</Mandatory>
        <Type>Objlink</Type>
        <RangeEnumeration></RangeEnumeration>
        <Units></Units>
        <Description>Update the current scene or the linked scene</Description>
      </Item>
      <Item ID="5750">
        <ResourceID>5750</ResourceID>
        <Name>Application Type</Name>
        <Operations>R</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Optional</Mandatory>
        <Type>String</Type>
        <RangeEnumeration></RangeEnumeration>
        <Units></Units>
        <Description>The application type or use context</Description>
      </Item>
    </Resources>
  </Object>
</LWM2M>
